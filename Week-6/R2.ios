enable
!delete vlan.dat
configure terminal
hostname R2
no ip domain-lookup
banner motd #Verboden toegang voor onbevoegde!#
line console 0
 exec-timeout 60 0
 logging synchronous
 exit
enable secret class
username user priv 15 password cisco
aaa new-model
aaa authentication login MGT local
line vty 0 4
 transport input ssh
 login authentication MGT
 exec-timeout 60
 logging synchronous
 exit
ip domain-name hhs.nl
!crypto key zeroize rsa
crypto key generate rsa general-keys modulus 2048

interface GigabitEthernet 0/0/0
ip address 40.0.255.254 255.255.255.252
no shutdown
exit

interface GigabitEthernet 0/0/1
ip address 20.0.2.1 255.255.255.0
no shutdown
exit

interface Serial 0/1/0
ip address 20.0.0.2 255.255.255.0
no shutdown
exit

router ospf 1
network 20.0.0.0 0.0.0.255 area 0
network 20.0.2.0 0.0.0.255 area 0
network 40.0.255.252 0.0.0.3 area 0

router bgp 120
bgp router-id 120.0.0.2
bgp log-neighbor-changes
neighbor 40.0.255.253 remote-as 140
neighbor 20.0.0.1 remote-as 120
network 20.0.0.0 mask 255.255.255.0
network 20.0.2.0 mask 255.255.255.0
network 40.0.255.252 mask 255.255.255.252
exit

end
clock set 12:05:49 11 Dec 2025

