enable
delete vlan.dat


configure terminal
hostname R1
no ip domain-lookup
banner motd #Verboden toegang voor onbevoegde!#
line console 0
 exec-timeout 60 0
 logging synchronous
 exit
enable secret class
username user priv 15 password cisco
aaa new-model
aaa authentication login MGT local
line vty 0 4
 transport input ssh
 login authentication MGT
 exec-timeout 60
 logging synchronous
 exit
ip domain-name hhs.nl
!crypto key zeroize rsa
crypto key generate rsa general-keys modulus 2024

!Port Config
interface Gi0/0/0
 ip address 10.1.0.1 255.255.255.252
 no shutdown
exit

interface Gi0/0/1
 ip address 10.2.0.1 255.255.255.252
 no shutdown
exit

interface loopback1
 ip address 10.10.10.10 255.255.255.255
 no shutdown
exit

!Miscellaneous System Config

!Ip Config

!OSPF Config
router ospf 1
 network 10.1.0.0 0.0.0.3 area 0
 network 10.2.0.0 0.0.0.3 area 0
 network 10.10.10.10 0.0.0.0 area 0
exit

!router DHCP
! DHCP for VLAN 10
ip dhcp pool VLAN10-POOL
  network 10.0.1.0 255.255.255.0
  default-router 10.0.1.3
  dns-server 8.8.8.8
  lease 0 12
  ! Exclude .1-.9 to prevent conflicts
ip dhcp excluded-address 10.0.1.1 10.0.1.9

! DHCP for VLAN 20
ip dhcp pool VLAN20-POOL
  network 10.0.2.0 255.255.255.0
  default-router 10.0.2.3
  dns-server 8.8.8.8
  lease 0 12
ip dhcp excluded-address 10.0.2.1 10.0.2.9


end
clock set 09:36:34 24 nov 2025
copy running-config startup-config
